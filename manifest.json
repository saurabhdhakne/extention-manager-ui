{
    "manifest_version": 2,
    "name": "BOOKTO",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "storage",
        "bookmarks",
        "background"
    ],
    "browser_action": {
        "default_title": "My Bookmarks",
        "default_popup": "popup.html"
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js https://use.fontawesome.com/db006bf474.js; object-src 'self'"
}